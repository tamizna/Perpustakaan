          <AuxValues>
            <AuxValue name="JavaCodeGenerator_CreateCodeCustom" type="java.lang.String" value="new javax.swing.JTable()"/>
            <AuxValue name="JavaCodeGenerator_InitCodePre" type="java.lang.String" value="// membuat tampilan model tabel&#xa;        model.addColumn(&quot;No&quot;);&#xa;        model.addColumn(&quot;NPM&quot;);&#xa;        model.addColumn(&quot;Nama Lengkap&quot;);&#xa;        model.addColumn(&quot;Jenis Kelamin&quot;);&#xa;        model.addColumn(&quot;Jurusan&quot;);&#xa;        model.addColumn(&quot;Tahun Angkatan&quot;);&#xa;        model.addColumn(&quot;Alamat&quot;);&#xa;&#xa;        //menampilkan data database ke dalam tabel&#xa;        try {&#xa;            int no = 1;&#xa;            String sql = &quot;select * from member&quot;;&#xa;            java.sql.Connection conn = (Connection) Config.configDB();&#xa;            java.sql.Statement stm = conn.createStatement();&#xa;            java.sql.ResultSet res = stm.executeQuery(sql);&#xa;            while (res.next()) {&#xa;                model.addRow(new Object[]{no++, res.getString(1), res.getString(2),&#xa;                    res.getString(3), res.getString(4), res.getString(5),&#xa;                    res.getString(6)});&#xa;            }&#xa;&#xa;        } catch (SQLException e) {&#xa;        }&#xa;&#xa;jTable1.setModel(model);"/>
          </AuxValues>
        </Component>
      </SubComponents>
